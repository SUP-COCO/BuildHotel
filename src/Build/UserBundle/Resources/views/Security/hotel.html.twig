		<script type="text/javascript">
		$(document).ready(function()
		{
			$('#fermerBarre').click(function()
			{
				$('#barreTexte').fadeOut();
				$('#barre').slideUp();
				$('#outilsJoueur').slideUp(function()
				{
					$('#barreReouvrir').fadeIn(function()
					{
						$('.ouvrirOutilsJoueur').hide();
						$('.fermerOutilsJoueur').show();
					});
				});
			});

			$('#barreReouvrir').click(function()
			{
				$('#barreReouvrir').fadeOut(function()
				{
					$('.fermerOutilsJoueur').hide();
					$('.ouvrirOutilsJoueur').show();
					$('#barreTexte').fadeIn();
					$('#barre').slideDown();
				});
			});

			$('.ouvrirOutilsJoueur').click(function()
			{
				$('#outilsJoueur').slideDown();
				$('.ouvrirOutilsJoueur').hide();
				$('.fermerOutilsJoueur').show();
			});

			$('.fermerOutilsJoueur').click(function()
			{
				$('#outilsJoueur').slideUp();
				$('.fermerOutilsJoueur').hide();
				$('.ouvrirOutilsJoueur').show();
			});			
		});
		</script>
