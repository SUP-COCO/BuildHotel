{% extends '::template.html.twig' %}
{% block title %}{{ parent() }} Compte{% endblock %}
  
{% block body %}
	<div id="wrapper">
		<table style="width:100%">
			{% for user in users %}
				<tr>
					<td>{{ user.username }}</td>
					<td>{{ user.mail }}</td>
					<td><a href="/admin/users/{{user.id}}">Modifier</a></td>
				</tr>
			{% endfor %}
		</table>
	</div>
{% endblock %}