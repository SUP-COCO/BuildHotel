<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="50" nullable="false"/>
    <field name="realName" type="string" column="real_name" length="100" nullable="false"/>
    <field name="password" type="string" column="password" length="50" nullable="false"/>
    <field name="mail" type="string" column="mail" length="50" nullable="false"/>
    <field name="authTicket" type="text" column="auth_ticket" length="65535" nullable="false"/>
    <field name="rank" type="integer" column="rank" nullable="false"/>
    <field name="credits" type="integer" column="credits" nullable="false"/>
    <field name="vipPoints" type="integer" column="vip_points" nullable="false"/>
    <field name="activityPoints" type="integer" column="activity_points" nullable="false"/>
    <field name="activityPointsLastupdate" type="float" column="activity_points_lastupdate" precision="10" scale="0" nullable="false"/>
    <field name="look" type="string" column="look" length="255" nullable="false"/>
    <field name="gender" type="string" column="gender" nullable="false"/>
    <field name="motto" type="string" column="motto" length="50" nullable="false"/>
    <field name="accountCreated" type="string" column="account_created" length="50" nullable="false"/>
    <field name="lastOnline" type="string" column="last_online" length="50" nullable="false"/>
    <field name="online" type="string" column="online" nullable="false"/>
    <field name="ipLast" type="string" column="ip_last" length="120" nullable="false"/>
    <field name="ipReg" type="string" column="ip_reg" length="120" nullable="false"/>
    <field name="homeRoom" type="integer" column="home_room" nullable="false"/>
    <field name="respect" type="integer" column="respect" nullable="false"/>
    <field name="dailyRespectPoints" type="integer" column="daily_respect_points" nullable="false"/>
    <field name="dailyPetRespectPoints" type="integer" column="daily_pet_respect_points" nullable="false"/>
    <field name="newbieStatus" type="integer" column="newbie_status" nullable="false"/>
    <field name="isMuted" type="string" column="is_muted" nullable="false"/>
    <field name="mutantPenalty" type="string" column="mutant_penalty" nullable="false"/>
    <field name="mutantPenaltyExpire" type="integer" column="mutant_penalty_expire" nullable="false"/>
    <field name="blockNewfriends" type="string" column="block_newfriends" nullable="false"/>
    <field name="hideOnline" type="string" column="hide_online" nullable="false"/>
    <field name="hideInroom" type="string" column="hide_inroom" nullable="false"/>
    <field name="mailVerified" type="string" column="mail_verified" length="6" nullable="false"/>
    <field name="vip" type="string" column="vip" nullable="false"/>
    <field name="volume" type="integer" column="volume" nullable="false"/>
    <field name="acceptTrading" type="string" column="accept_trading" nullable="false"/>
  </entity>
</doctrine-mapping>
